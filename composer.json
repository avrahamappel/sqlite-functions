{
    "name": "appel/sqlite-functions",
    "description": "A polyfill for various SQL functions that SQLite doesn't natively support.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "avrahamappel",
            "email": "aappel248@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-pdo": "*",
        "illuminate/database": "^6.20.14 || ^7.30.4 || ^8.24.0",
        "illuminate/support": "^6.20.14 || ^7.30.4 || ^8.24.0"
    },
    "autoload": {
        "psr-4": {
            "Appel\\SqliteFunctions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appel\\SqliteFunctions\\ServiceProvider"
            ]   
        }
    }
}
